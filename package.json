  "description": "Image processing api that use express as backend and retrieve images for different sizes on the web",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && npm run jasmine",
    "build": "npx tsc",
    "jasmine": "jasmine",
    "start": "nodemon src/index.ts",
    "lint": "eslint . src/**/*.ts",
    "lint:fix": "eslint .  src/**/*.ts --fix",
    "format": "prettier src/**/*.ts --write "
  },
  "keywords": [
    "image",
    "express",
    "api"
  ],
  "author": "Daniel Valencia",
  "license": "MIT",
